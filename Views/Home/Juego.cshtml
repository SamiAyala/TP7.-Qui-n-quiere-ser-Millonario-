<nav class="navbar">
    <a class="logoNav" href='@Url.Action("Index", "Home")'>
      <img src="/assets/logo.webp" class="img-fluid">
    </a>
    <div class="cont-comodines">
        <img src="/assets/comodin1.png" class="imgComodin mx-5">
        <img src="/assets/comodin2.png" class="imgComodin">
    </div>
</nav>
<div class="row justify-content-center">
    <div class="col-3 mb-5">
        <img class="img-fluid" src="/assets/logo.webp"/>
    </div>
</div>
<div class="row justify-content-center">
    <div class="col-md-auto cont-preg-resp w-50 rounded-3 p-4">
        <div class="preg-resp align-middle fs-3" id="pregunta">@ViewBag.listPreg[1].textoPregunta</div>
    </div>
</div>
<div class="row justify-content-center mt-5">
    <div class="col-4 cont-preg-resp w-25 rounded-3 mx-5 p-2">
        <div class="preg-resp align-middle fs-5" id="respuesta1">@ViewBag.listResp[1].</div>
    </div>
    <div class="col-4 cont-preg-resp w-25 rounded-3 mx-5 p-2">
        <div class="preg-resp align-middle fs-5" id="respuesta2">Respuesta 2</div>
    </div>
</div>
<div class="row justify-content-center mt-5">
    <div class="col-4 cont-preg-resp w-25 rounded-3 mx-5 p-2">
        <div class="preg-resp align-middle fs-5" id="respuesta3">Respuesta 3</div>
    </div>
    <div class="col-4 cont-preg-resp w-25 rounded-3 mx-5 p-2">
        <div class="preg-resp align-middle fs-5" id="respuesta4">Respuesta 4</div>
    </div>
</div>

<table class="table position-absolute top-50 end-0 translate-middle-y text-center" style="width:10%; margin-right:2%; background-color: rgb(5, 5, 54); color: #FFFFFF; border: 3px solid #FFB949;">
    <tbody>
        @foreach (Pozo p in @ViewBag.listPozo)
        {
            if(p.valorSeguro){
				<th scope="row" style="color:#FFB949;">$@p.importe</th>
            } else{
                <th scope="row">$@p.importe</th>
            }
            <tr></tr>
        }
    </tbody>
</table>
